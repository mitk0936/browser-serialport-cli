{
  "name": "serial-web-cli",
  "version": "1.0.0",
  "description": "A simple web console for testing and debugging hm-10 BLE device via serial port.",
  "main": "server/index.js",
  "scripts": {
    "test": "",
    "start": "opn http://localhost:6543 && PORT=6543 node --inspect server/index",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors"
  },
  "keywords": [
    "ble",
    "hm-10",
    "node.js",
    "web",
    "bluetooth",
    "usb",
    "serial",
    "javascript"
  ],
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-console-component": "^0.6.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-toolbox": "^2.0.0-beta.13",
    "redux": "^3.7.2",
    "redux-freeze": "^0.1.5",
    "redux-saga": "^0.16.0",
    "serialport": "^6.2.2",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.19.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "opn-cli": "^3.1.0",
    "postcss-loader": "^0.6.0",
    "rimraf": "^2.4.3",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
