{
  "name": "serial-web-cli",
  "version": "1.0.0",
  "description": "A simple web console for testing and debugging hm-10 BLE device via serial port.",
  "main": "server/index.js",
  "scripts": {
    "test": "",
    "start": "opn http://localhost:6543 && PORT=6543 node --inspect server/index",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors"
  },
  "browserslist": "> 0.25%, not dead",
  "keywords": [
    "ble",
    "hm-10",
    "node.js",
    "web",
    "bluetooth",
    "usb",
    "serial",
    "javascript"
  ],
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-console-component": "^0.6.1",
    "react-dom": "^16.9.0",
    "serialport": "^8.0.7",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^6.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^1.0.7",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.13.0",
    "npm": "^6.14.6",
    "opn-cli": "^3.1.0",
    "postcss-loader": "^0.6.0",
    "rimraf": "^2.7.1",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "webpack": "^4.40.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "author": "Dimitar Mihaylov",
  "license": "MIT"
}
